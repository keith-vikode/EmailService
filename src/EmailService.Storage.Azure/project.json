{
  "version": "1.0.0-*",

  "buildOptions": {
    "debugType": "portable",
    "warningsAsErrors": true,
    "additionalArguments": [ "/ruleset:../../stylecop.ruleset", "/additionalfile:../../stylecop.json" ]
  },

  "dependencies": {
    "EmailService.Core": "1.0.0-*",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "WindowsAzure.Storage": "8.1.1"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  }
